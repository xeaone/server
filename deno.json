{
    "name": "@xeaone/server",
    "version": "5.3.2",
    "exports": "./src/mod.ts",
    "tasks": {
        "t": "deno test -A",
        "d": "deno run -A deploy.ts",
        "p": "deno publish --allow-slow-types",
        "e": "deno run -A --watch examples/server.ts"
    },
    "lint": {
        "include": [ "." ],
        "exclude": [ "examples" ],
        "rules": {
            "exclude": [ "no-explicit-any" ]
        }
    },
    "fmt": {
        "include": [ "." ],
        "exclude": [ "examples" ],
        "options": {
            "useTabs": false,
            "lineWidth": 800,
            "indentWidth": 4,
            "singleQuote": true,
            "proseWrap": "preserve"
        }
    }
}
